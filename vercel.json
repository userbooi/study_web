{
    "version": 2,
    "builds": [
        {
            "src": "study_web/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
            "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/my_static_files/(.*)",
            "dest": "/my_static_files/$1"
        },
        {
            "src": "/(.*)",
            "dest": "study_web/wsgi.py"
        }
    ]
}